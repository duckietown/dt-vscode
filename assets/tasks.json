{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build solution",
            "type": "shell",
            "command": "dts devel build -f -H ${HOSTNAME}"
        },
        {
            "label": "run solution",
            "type": "shell",
            "command": "dts devel run -H ${HOSTNAME}"
        },
        {
            "label": "stop solution",
            "type": "shell",
            "command": "docker -H ${HOSTNAME} restart -t 1 duckiebot-interface"
        }
    ]
 }
 